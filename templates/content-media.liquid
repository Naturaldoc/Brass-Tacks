{% block content %} 
<section class="content">
	<h2>Sample Media Content</h2>
	<p>
		Various examples of media/media-text content groupings.
	</p>

<h3>Images</h3>
<div class="row">
	<div class="col col-60">
		<h3>Default Image</h3>
		<img src="http://placedog.com/280/280" alt="Dog">
		
		<h3>Linked Image</h3>
		<a href="#" class="pic">
			<img src="assets/img/little-V.jpg" alt="not rounded" />
		</a>
		
		<a href="#" class="pic-Rounded">
			<img src="assets/img/little-V.jpg" alt="rounded pic" />
		</a>
		
		<h3>Missing Image</h3>
		<img src="#" alt="This is an example of a missing image">
		
		
		<h3>Svg</h3>
		<svg width="200px" height="200px">
		  <circle cx="100" cy="100" r="100" fill="#ff0000" />
		</svg>
	</div> <!-- end .col-60 -->
	
	<div class="col col-40">

			explain

	</div>
</div> <!-- end .row -->


<h3>Audio &amp; Video</h3>
<div class="row">
	<div class="col col-60">
		<h3>Video</h3>
		<video controls="controls" preload="auto" poster="http://sandbox.thewikies.com/vfe-generator/images/big-buck-bunny_poster.jpg" width="640" height="360">
		  <source id="mp4" src="http://media.w3.org/2010/05/bunny/trailer.mp4" type="video/mp4">
		  <source id="ogv" src="http://media.w3.org/2010/05/bunny/trailer.ogv" type="video/ogg">
		  <p>Your user agent does not support the HTML5 Video element.</p>
		</video>
		
		<h3>Missing Video</h3>
		<video controls="controls"></video>
		
		
		<h3>Audio</h3>
		<audio controls="controls">
		 <source src="http://media.w3.org/2010/07/bunny/04-Death_Becomes_Fur.mp4" type='audio/mp4'>
		 <source src="http://media.w3.org/2010/07/bunny/04-Death_Becomes_Fur.oga" type='audio/ogg; codecs=vorbis'>
		 <p>Your user agent does not support the HTML5 Audio element.</p>
		</audio>
		
		<h3>Missing Audio</h3>
		<audio controls="controls"></audio>
	</div> <!-- end .col-60 -->
	
	<div class="col col-40">

			explain

	</div>
</div> <!-- end .row -->

</section>   	
{% endblock %}




